{
    "presets": [
      "@babel/env",
      "@babel/react",
      "@babel/typescript"
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "es",
          "style": "css"
        }
      ], // `style: true` 会加载 less 文件
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      [
        "@babel/proposal-class-properties",
        {
          "legacy": true
        }
      ]
    ]
  }